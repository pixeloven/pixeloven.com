{
  "name": "pixeloven.com",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "e2e": "turbo run e2e",
    "e2e:headless": "turbo run e2e:headless",
    "component": "turbo run component",
    "component:headless": "turbo run component:headless",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "turbo": "^2.4.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {},
  "packageManager": "npm@10.9.2"
}
